{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".output/**", "build/**"],
      "outputLogs": "full"
    },
    "dev": {
      "cache": false,
      "env": ["APP_ENV=development"],
      "persistent": true,
      "outputLogs": "full"
    }
  }
}
