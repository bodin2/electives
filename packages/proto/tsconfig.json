{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "types": ["bun-types"],

    "noEmit": true,

    "strict": true,
    "forceConsistentCasingInFileNames": true,

    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
